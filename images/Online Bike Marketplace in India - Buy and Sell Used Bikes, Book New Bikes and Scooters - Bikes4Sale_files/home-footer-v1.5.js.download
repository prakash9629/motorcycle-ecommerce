function goToComp(){
var link = '/compare';
var a = '/';
var comp1 = $('#company1').val();
var comp2 = $('#company2').val();
var modl1 = $('#model1').val();
var modl2 = $('#model2').val();
var c1 = getSEOUrl(($('#company1').val() || '') + ' ' + ($('#model1').val() || ''));
var c2 = getSEOUrl(($('#company2').val() || '') + ' ' + ($('#model2').val() || ''));
