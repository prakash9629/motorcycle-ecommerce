location.href = getSearchLinkAdv($(itemType).val(), $(newCompany).val(), '');
}catch(e){
}
}
function setBc(company,selVal){
if(!company) return false;
try{
var options='<option value="">Select Brand</option>';
for(var ele in specArrModel){
options+='<option value="'+ele+'" ';
if(selVal && selVal==ele) options+='selected="selected"';
options+='>'+ele+'</option>';
}
var mdlArr=specArrModel[$(company).val()];
if(mdlArr && mdlArr.length==0)
$('.item-model-box').css('visibility','hidden');
else
$('.item-model-box').css('visibility','visible');
if($(company).val()=='Other' || selVal=='Other' ||(mdlArr && mdlArr.length==0)){
$('.other-full-name').text('');
$('.other-box').fadeIn('slow');
}
$(company).html(options);
} catch(e){
}
}
function setMc(company,model,version,bYear,selVal){
try{
if(!company || !model) return false;
var options='<option value="">Select Model</option>';
var mdlArr=specArrModel[$(company).val()];
if(mdlArr){
for(var k=0;k < mdlArr.length;k++){
options+='<option value="'+mdlArr[k]+'" ';
if(selVal && selVal==mdlArr[k]) options+='selected="selected"';
options+='>'+mdlArr[k]+'</option>';
}
}
if(mdlArr && mdlArr.length==0)
$('.item-model-box').css('visibility','hidden');
else
$('.item-model-box').css('visibility','visible');
if($(company).val()=='Other' || $(model).val()=='Other' || selVal=='Other' ||(mdlArr && mdlArr.length==0)){
$('.other-full-name').text('');
$('.other-box').fadeIn('slow');
}else{
$('.other-box').hide();
}
$(model).html(options);
} catch(e){
}
}
function setComparePic(company,model,pic,nam){
try{
if(!company || !model || !pic || !nam) return false;
var itemName = $(company).val() + ' ' + $(model).val();
var imgSrc = '/pictures/default/'+getSEOUrl(itemName)+'/'+getSEOUrl(itemName)+'-640.jpg';
$(pic).prop('src',imgSrc);
$(pic).prop('alt',itemName);
$(nam).text(itemName);
}catch(e){
}
}
