function usedadvSearch(stat,budg){
var link = '/used/buy/';
var defItemName = "bikes";
if(trim($(stat).val())){link += defItemName + "-in-" + getSEOUrl(trim($(stat).val())) + "/";}
if(trim($(budg).val())){link += '?price=' + trim($(budg).val());}
location.href = link;
}
function setBrand(itemType,newCompany){
var selType = $(itemType).val();
var options='<option value="">Select Brand</option>';
var mdlArr=newArrModel[selType];
if(mdlArr){
for(var k=0;k < mdlArr.length;k++){
options+='<option value="'+getSEOUrl(mdlArr[k])+'" ';
options+='>'+mdlArr[k]+'</option>';
}
}
$(newCompany).html(options);
}
function newadvSearch(itemType,newCompany){
